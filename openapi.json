openapi: 3.0.1
info:
  title: RefreshHub Info API
  version: 1.0.0
paths:
  /services:
    get:
      summary: List vending services
      responses:
        '200':
          description: OK
          content:
            application/json:
              schema: 
                type: array
                items:
                  $ref: '#/components/schemas/Service'
components:
  schemas:
    Service:
      type: object
      properties:
        name:
          type: string
        description:
          type: string
